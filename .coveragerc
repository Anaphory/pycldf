[run]
branch = True
source = pycldf

[paths]
source =
    src/pycldf
    .tox/*/lib/python*/site-packages/pycldf

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*
show_missing = True

